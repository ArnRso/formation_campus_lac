{% extends 'base.html.twig' %}

{% block body %}
    {% for book in books %}
        <article>
            <h2>
                {{ book.title }}
            </h2>
            <p>
                Ecrit par {{ book.author }}
            </p>
            <p>ISBN : {{ book.isbn }}</p>
        </article>
    {% endfor %}
{% endblock body %}